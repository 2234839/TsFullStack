{
	"name": "tsfullstack-backend",
	"type": "module",
	"scripts": {
		"build": "tsup --config tsup-build.config.ts",
		"build:publish": "pnpm --filter admin-panel build && pnpm build && pnpm publish:server",
		"publish:server": "bash ./publish.sh",
		"tsc:b": "tsc -b",
		"dev": "tsup --watch & pnpm start:w",
		"start": "node --enable-source-maps ./dist/index.cjs",
		"start:w": "nodemon --enable-source-maps ./dist/index.cjs",
		"build:lib": "tsup --config tsup-lib.config.ts && pnpm build:lib:dmap",
		"build:lib:dmap": "tsc ./src/lib/index.ts --emitDeclarationOnly --declaration --declarationMap --outDir ./dist-lib",
		"build:lib:w": "tsup --watch --config tsup-lib.config.ts"
	},
	"main": "./dist-lib/index.js",
	"types": "./dist-lib/lib/index.d.ts",
	"files": [
		"dist-lib",
		"./src/lib/index.ts"
	],
	"devDependencies": {
		"@types/node": "^22.14.1",
		"nodemon": "^3.1.9",
		"prisma": "^6.6.0",
		"regexp-to-ast": "^0.5.0",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3",
		"zenstack": "2.14.0"
	},
	"dependencies": {
		"@fastify/cors": "^11.0.1",
		"@fastify/multipart": "^9.0.3",
		"@fastify/static": "^8.1.1",
		"@prisma/client": "6.6.0",
		"@zenstackhq/runtime": "2.14.0",
		"bcryptjs": "^3.0.2",
		"effect": "^3.14.12",
		"fastify": "^5.3.2",
		"superjson": "^2.2.2",
		"uuid": "^11.1.0"
	}
}