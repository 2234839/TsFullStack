{
  "name": "info-flow",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "tsc": "vue-tsc",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "release": "pnpm build && pnpm build:firefox && pnpm zip && pnpm zip:firefox",
    "release:version": "npm version patch",
    "release:prepare": "pnpm install && pnpm tsc && pnpm release"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.3",
    "@tailwindcss/vite": "^4.1.12",
    "@vueuse/core": "^13.8.0",
    "@webext-core/proxy-service": "^1.2.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.0",
    "fast-equals": "^5.2.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.7",
    "superjson": "^2.2.2",
    "tailwindcss": "^4.1.12",
    "tsfullstack-backend": "workspace:*",
    "vue": "^3.5.20"
  },
  "devDependencies": {
    "@wxt-dev/module-vue": "^1.0.2",
    "typescript": "5.9.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.6",
    "wxt": "^0.20.8"
  }
