{
  "name": "@tsfullstack/module-template",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./api": {
      "import": "./dist/api/api.js",
      "require": "./dist/api/api.umd.cjs",
      "types": "./dist/api/types/index.d.ts"
    },
    "./frontend": {
      "import": "./dist/frontend/frontend.js",
      "require": "./dist/frontend/frontend.umd.cjs",
      "types": "./dist/frontend/types/index.d.ts"
    },
    "./routeMap": {
      "import": "./dist/frontend/routeMap.js",
      "types": "./dist/frontend/types/src/routeMap.d.ts"
    }
  },
  "files": [
    "dist",
    "api",
    "frontend"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:frontend && pnpm run build:api",
    "build:frontend": "vite build --config vite.frontend.config.ts",
    "build:api": "vite build --config vite.api.config.ts",
    "build:all": "pnpm run build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:api": "tsc --noEmit --project tsconfig.api.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tsfullstack/shared-frontend": "workspace:*"
  },
  "peerDependencies": {
    "vue": "catalog:",
    "vue-router": "catalog:",
    "primevue": "catalog:",
    "primeicons": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "catalog:",
    "unplugin-vue-components": "^29.1.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "^3.0.7"
  },
  "packageManager": "pnpm@10.17.1"
}
