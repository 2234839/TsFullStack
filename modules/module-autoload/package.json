{
  "name": "@tsfullstack/module-autoload",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/generated/api.js",
      "types": "./dist/generated/api.d.ts"
    },
    "./frontend": {
      "import": "./dist/generated/frontend.js",
      "types": "./dist/generated/frontend.d.ts"
    },
    "./routes": {
      "import": "./dist/generated/frontend.js",
      "types": "./dist/generated/frontend.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx automate-build.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tsfullstack/module-template": "workspace:*",
    "@tsfullstack/module-loader": "workspace:*",
    "@tsfullstack/shared-frontend": "workspace:*"
  },
  "devDependencies": {
    "fast-glob": "^3.3.2",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "turbo": "^1.12.3",
    "typescript": "~5.8.3"
  }
}
